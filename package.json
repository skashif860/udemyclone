        "vue-chartkick": "^0.5.2",
        "vue-clipboard2": "^0.3.0",
        "vue-element-loading": "^1.1.5",
        "vue-js-toggle-button": "^1.3.2",
        "vue-moment": "^4.0.0",
        "vue-nestable": "^2.4.5",
        "vue-prism-editor": "^0.3.0",
        "vue-quill-editor": "^3.0.6",
        "vue-slick": "^1.1.15",
        "vue-star-rating": "^1.6.1",
        "vue-tables-2": "^1.4.70",
        "vue-wysiwyg": "^1.7.2",
        "vue2-editor": "^2.10.2",
